{
  "extends": [
    "tslint-microsoft-contrib",
    "tslint-react",
    "tslint-origin-ordered-imports-rule"
  ],
  "rulesDirectory": [
    "node_modules/tslint-origin-ordered-imports-rule/dist"
  ],
  "rules": {
    "completed-docs": false,
    "export-name": false,
    "import-name": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": true,
    "max-line-length": [true, { "limit": 140, "ignore-pattern": "^import |^export {(.*?)}" }],
    "member-access": false,
    "missing-jsdoc": false,
    "no-any": false,
    "no-default-export": false,
    "no-implicit-dependencies": [true, ["!file-loader!assets"]],
    "no-floating-promises": false,
    "no-relative-imports": false,
    "no-require-imports": false,
    "no-submodule-imports": false,
    "no-unsafe-any": false,
    "ordered-imports": false,
    "origin-ordered-imports": [true, "one-blank-line"],
    "quotemark": [true, "single", "jsx-double"],
    "strict-boolean-expressions": false,
    "trailing-comma": true,
    "typedef": false,
    "variable-name": [true, "allow-pascal-case", "ban-keywords", "check-format"]
  }
}
